@model IEnumerable<SIFCA.Models.OBJETIVOINVENTARIO>

@{
    ViewBag.Title = "Gestionar objetivo inventario";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<a href="@Url.Action("Create", "OBJETIVOINVENTARIO")" class="btn btn-primary"><i class="fa fa-plus "></i> Crear objetivo inventario</a>
<br />
<br />
<div class="panel panel-default">
    <div class="panel-heading">
        Listado objetivo inventario
    </div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover" id="dataTables">
                <thead>
                    <tr>
                        <th>
                            Objetivo inventario
                        </th>
                        <th>
                            Descripcion Objetivo inventario
                        </th>
                        <th></th>
                        <th></th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr class="gradeA">
                            <td>
                                @Html.DisplayFor(modelItem => item.NOMBRETIPOINV)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.DESCRIPOBJETINV)
                            </td>
                            <td><a href="@Url.Action("Edit", "OBJETIVOINVENTARIO", new { id = item.NOMBRETIPOINV })"><i class="fa fa-edit "></i>Editar</a></td>
                            <td><a href="@Url.Action("Details", "OBJETIVOINVENTARIO", new { id = item.NOMBRETIPOINV })"><i class="fa fa-search "></i>Ver detalles</a></td>
                            <td><a href="@Url.Action("Delete", "OBJETIVOINVENTARIO", new { id = item.NOMBRETIPOINV })" class="delete-link"><i class="fa fa-pencil"></i>Eliminar</a></td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
